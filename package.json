{
  "name": "goto-assistant",
  "version": "0.0.0",
  "description": "Lightweight, self-hosted personal AI assistant",
  "license": "MIT",
  "packageManager": "pnpm@10.29.3",
  "type": "module",
  "bin": {
    "goto-assistant": "./bin/goto-assistant.js",
    "goto-assistant-messaging": "./bin/goto-assistant-messaging.js"
  },
  "files": [
    "dist/",
    "public/",
    "bin/"
  ],
  "engines": {
    "node": ">=20.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jolks/goto-assistant"
  },
  "keywords": [
    "ai",
    "assistant",
    "chat",
    "claude",
    "openai",
    "mcp",
    "self-hosted"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.0",
    "@openai/agents": "^0.4.0",
    "@openai/agents-extensions": "^0.4.0",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "better-sqlite3": "^11.0.0",
    "express": "^5.0.0",
    "link-preview-js": "^3.2.0",
    "multer": "^2.0.2",
    "qrcode": "^1.5.4",
    "ws": "^8.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/ws": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.2",
    "jsdom": "^28.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}
